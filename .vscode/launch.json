    "version": "0.2.0",
    "configurations": [
        {
            "name": "PyTest Current File",
            "type": "debugpy",
            "request": "launch",
            "module": "pytest",
            "args": [
                "${file}",
                "-s",
                "-x",
                "-k",
                "levenshtein"
            ],
            "console": "integratedTerminal",
            "justMyCode": false
        }
    ]
}